# Application
NODE_ENV=development
PORT=3000

# Database
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USERNAME=eventify_user
DATABASE_PASSWORD=eventify_password
DATABASE_NAME=eventify_db

# Google Cloud Storage
GOOGLE_CLOUD_PROJECT_ID=your-project-id
GOOGLE_CLOUD_STORAGE_BUCKET=your-bucket-name
GOOGLE_APPLICATION_CREDENTIALS=./service-account-key.json

# Qdrant Cloud Configuration
QDRANT_URL=https://your-cluster-id.qdrant.tech:6333
QDRANT_API_KEY=your-qdrant-api-key

# Sui Blockchain Configuration (Testnet)
# Generate a new keypair and fund it with testnet SUI
# Visit: https://docs.sui.io/guides/developer/getting-started/get-coins
SUI_PRIVATE_KEY=
SUI_PACKAGE_ID=
SUI_NETWORK=testnet 